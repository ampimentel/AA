np.cov(vect.T)\n","    d = multivariate_normal(mean = mean_vect, cov = cov_vect)\n","    return d\n","\n","d1 = create_distribution(class_1)\n","d2 = create_distribution(class_2)\n","d3 = create_distribution(class_3)\n","\n","def best_class(x_t, d1, d2, d3, pc_1, pc_2, pc_3):\n","    prob1 = d1.pdf(x_t) * pc_1\n","    prob2 = d2.pdf(x_t) * pc_2\n","    prob3 = d3.pdf(x_t) * pc_3\n","\n","    if prob1 >= prob2 and prob1 >= prob3:\n","        return 1\n","    elif prob2 >= prob1 and prob2 >= prob3:\n","        return 2\n","    elif prob3 >= prob1 and prob3 >= prob2:\n","        return 3\n","    else:\n","        return 0\n","\n","y_predicted = []\n","for x in x_test:\n","    y_predicted.append(best_class(x, d1, d2, d3, pc_1, pc_2, pc_3))\n","\n","count = 0\n","for y1, y2 in zip(y_test, y_predicted):\n","    if y1 != y2:\n","        count += 1\n","\n","error_bayes = 100 * (count / y_test.size)\n","print(\"Error bayes: %{}\".format(error_bayes))\n","acc_score = accuracy_score(y_test, y_predicted)\n","print(\"Accuracy Score: {}\".format(acc_score))\n"],"execution_count":null,"outputs":[{"output_type":"stream","text":["Error bayes: %3.3333333333333335\n","Accuracy Score: 0.9666666666666667\n"],"name":"stdout"}]},{"cell_type":"markdown","metadata":{"id":"FTBQ2ReiBsXV"},"source":["## 2.7 Comparative Plots\n","\n"]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":299},"id":"W8xzDkP1CRr7","executionInfo":{"status":"ok","timestamp":1605960027295,"user_tz":0,"elapsed":1189,"user":{"displayName":"Afonso Pimentel","photoUrl":"","userId":"03608117926409301318"}},"outputId":"212d46d6-797c-44e3-f1c7-dba011011d60"},"source":["fig, (ax1, ax2, ax3) = plt.subplots(1, 3)\n","ax1.set_title('True Data')\n","ax2.set_title('Predicted Naive')\n","ax3.set_title('Predicted Bayes')\n","g1 = ax1.scatter(x_test[:, 0], x_test[:, 1], c = y_test, cmap = matplotlib.colors.ListedColormap(class_colors))\n","ax1.legend(*g1.legend_elements(), loc=\"upper left\", titl